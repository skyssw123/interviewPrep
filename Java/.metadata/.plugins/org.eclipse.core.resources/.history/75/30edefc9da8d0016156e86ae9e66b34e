
public class Main {
	
	static void setZeros(int[][] matrix)
	{
		
	}
	
	static int[][] generateMatrix()
	{
		int[][] matrix = new int[4][4];
		
		for(int i = 0; i < matrix.length ; i++)
		{
			for(int j = 0; j < matrix[0].length; j++)
			{
				matrix[i][j] = 1; 
			}
		}
		return matrix;
	}

	public static void main(String[] args) {
		int[][] a = generateMatrix();
	}
}
